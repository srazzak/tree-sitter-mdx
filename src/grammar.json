{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "mdx",
  "rules": {
    "document": {
      "type": "REPEAT",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SYMBOL",
            "name": "jsx_component"
          },
          {
            "type": "SYMBOL",
            "name": "markdown"
          },
          {
            "type": "SYMBOL",
            "name": "expression"
          }
        ]
      }
    },
    "jsx_component": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "jsx_opening_tag"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "jsx_content"
          }
        },
        {
          "type": "SYMBOL",
          "name": "jsx_closing_tag"
        }
      ]
    },
    "jsx_opening_tag": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "<"
        },
        {
          "type": "FIELD",
          "name": "name",
          "content": {
            "type": "SYMBOL",
            "name": "tag_name"
          }
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "attribute"
          }
        }
      ]
    },
    "jsx_closing_tag": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "/>"
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "</"
            },
            {
              "type": "FIELD",
              "name": "name",
              "content": {
                "type": "SYMBOL",
                "name": "tag_name"
              }
            },
            {
              "type": "STRING",
              "value": ">"
            }
          ]
        }
      ]
    },
    "tag_name": {
      "type": "PATTERN",
      "value": "[A-Za-z][A-Za-z0-9]*"
    },
    "attribute": {
      "type": "SEQ",
      "members": [
        {
          "type": "PATTERN",
          "value": "[A-Za-z][A-Za-z0-9]*"
        },
        {
          "type": "STRING",
          "value": "="
        },
        {
          "type": "PATTERN",
          "value": "\"[^\"]*\""
        }
      ]
    },
    "jsx_content": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "markdown"
        },
        {
          "type": "SYMBOL",
          "name": "jsx_component"
        },
        {
          "type": "SYMBOL",
          "name": "expression"
        }
      ]
    },
    "expression": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "{"
        },
        {
          "type": "PATTERN",
          "value": "[^}]+"
        },
        {
          "type": "STRING",
          "value": "}"
        }
      ]
    },
    "inline_jsx": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "jsx_opening_tag"
        },
        {
          "type": "SYMBOL",
          "name": "jsx_closing_tag"
        }
      ]
    },
    "markdown": {
      "type": "PATTERN",
      "value": "[^<{]+"
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}