{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "mdx",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SYMBOL",
            "name": "jsx_component"
          },
          {
            "type": "SYMBOL",
            "name": "markdown"
          },
          {
            "type": "SYMBOL",
            "name": "expression"
          }
        ]
      }
    },
    "jsx_component": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "<"
        },
        {
          "type": "SYMBOL",
          "name": "identifier"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "attribute"
          }
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "STRING",
              "value": "/>"
            },
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "STRING",
                  "value": ">"
                },
                {
                  "type": "REPEAT",
                  "content": {
                    "type": "SYMBOL",
                    "name": "jsx_content"
                  }
                },
                {
                  "type": "STRING",
                  "value": "</"
                },
                {
                  "type": "SYMBOL",
                  "name": "identifier"
                },
                {
                  "type": "STRING",
                  "value": ">"
                }
              ]
            }
          ]
        }
      ]
    },
    "identifier": {
      "type": "PATTERN",
      "value": "[A-Za-z][A-Za-z0-9]*"
    },
    "attribute": {
      "type": "SEQ",
      "members": [
        {
          "type": "PATTERN",
          "value": "[A-Za-z][A-Za-z0-9]*"
        },
        {
          "type": "STRING",
          "value": "="
        },
        {
          "type": "PATTERN",
          "value": "\"[^\"]*\""
        }
      ]
    },
    "jsx_content": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "markdown"
        },
        {
          "type": "SYMBOL",
          "name": "jsx_component"
        },
        {
          "type": "SYMBOL",
          "name": "expression"
        }
      ]
    },
    "expression": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "{"
        },
        {
          "type": "PATTERN",
          "value": "[^}]+"
        },
        {
          "type": "STRING",
          "value": "}"
        }
      ]
    },
    "inline_jsx": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "<"
        },
        {
          "type": "SYMBOL",
          "name": "identifier"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "attribute"
          }
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "STRING",
              "value": "/>"
            },
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "STRING",
                  "value": ">"
                },
                {
                  "type": "REPEAT",
                  "content": {
                    "type": "SYMBOL",
                    "name": "jsx_content"
                  }
                },
                {
                  "type": "STRING",
                  "value": "</"
                },
                {
                  "type": "SYMBOL",
                  "name": "identifier"
                },
                {
                  "type": "STRING",
                  "value": ">"
                }
              ]
            }
          ]
        }
      ]
    },
    "markdown": {
      "type": "PATTERN",
      "value": "[^<{]+"
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}